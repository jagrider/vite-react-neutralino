// TODO: Update
{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:all", "plugin:react/all", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/function-component-definition": [
      2,
      {
        "namedComponents": "arrow-function"
      }
    ]
  }
}